  },
  "devDependencies": {
    "@rjsf/core": "^2.4.0",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.52",
    "@types/react-jsonschema-form": "^1.7.4",
    "@vercel/node": "^1.9.0",
    "@sendgrid/mail": "^7.4.2",
    "next-sitemap": "^1.5.18",
    "prettier": "^2.2.1",
    "react-spinners": "^0.9.0",
    "sharp": "^0.27.2",
    "typescript": "^4.0.3",
    "classnames": "^2.2.6",
