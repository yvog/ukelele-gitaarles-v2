{
  "name": "ukelele-gitaarles-v2",
  "version": "1.0.0",
  "description": "Ukelele-Gitaarles official website v2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run gen:sitemap",
    "start": "next start",
    "export": "next export",
    "format": "prettier --write \"src/**/*.{ts,js,json,scss,css}\" ",
    "cleanup": "rimraf node_modules",
    "gen:sitemap": "next-sitemap --config sitemap.config.js",
    "prod:cleanup": "rm -rf package-lock.json ./public/sitemap.js",
    "prod:restart": "sudo forever restart",
    "prod:stop": "sudo forever stopall",
    "prod:start": "sudo forever start -c 'npx next start -p 8080' ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yvog/ukelele-gitaarles-v2.git"
  },
  "author": "Yvo Geldhof",
  "bugs": {
    "url": "https://github.com/yvog/ukelele-gitaarles-v2/issues"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.2",
    "@vercel/node": "^1.12.1",
    "next": "^12.0.7",
    "next-images": "^1.8.4",
    "swiper": "^7.4.1"
  },
  "devDependencies": {
    "@rjsf/core": "^2.4.0",
    "@types/classnames": "^2.3.1",
    "@types/node": "^14.11.8",
    "@types/react": "^17.0.38",
    "@types/react-jsonschema-form": "^1.7.4",
    "classnames": "^2.3.1",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "next-sitemap": "^1.5.18",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^5.0.3",
    "react-spinners": "^0.9.0",
    "sass": "^1.26.11",
    "sharp": "^0.27.2",
    "typescript": "^4.3.2"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  }
}
